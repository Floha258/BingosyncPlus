{
  "manifest_version": 2,
  "name": "Bingosync+",
  "version": "1.0.1",
  "description": "Provides a couple improvements to Bingosync",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.bingosync.com/room/*"
      ],
      "css": [
        "invasion.css"
      ],
      "js": [
        "invasion.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Bingosync+",
    "default_popup": "browserAction/settings.html"
  },
  "permissions": [
    "storage",
    "browsingData"
  ]
}
